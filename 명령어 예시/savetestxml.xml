<orders>
  <order name="계산" description="계산합니다">
    <orderUnit>
      <var>A,B</var>
      <input>$A 더하기 $B</input>
      <PSOrder>$A+$B</PSOrder>
      <voiceOutput>결과는 $R입니다.</voiceOutput>
    </orderUnit>
    <orderUnit>
      <var>B,C</var>
      <input>$B 빼기 $C</input>
      <PSOrder>$B-$C</PSOrder>
      <voiceOutput>결과는 $R입니다.</voiceOutput>
    </orderUnit>
    <orderUnit>
      <var>A</var>
      <input>$A 제곱근</input>
      <PSOrder>[math]::sqrt($A)</PSOrder>
      <voiceOutput>결과는 $R입니다.</voiceOutput>
    </orderUnit>
  </order>
</orders>